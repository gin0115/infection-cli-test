{
    "name": "gin0115/infection-test",
    "type": "library",
    "description": "Simple lib used to try and get Infection working with Stryker",
    "keywords": [],
    "license": "MIT",

    "autoload": {
        "psr-4": {
            "Gin0115\\Infection_Test\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gin0115\\Infection_Test\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "1.*",
        "symfony/var-dumper": "*",
        "infection/infection": "^0.26.6"
    },
    "require": {
        "php": ">=7.2.0"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}